{
  "expo": {
    "name": "Sync",
    "slug": "Sync",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "sync",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "xyz.justpatrick.sync"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FF0051"
      },
      "package": "xyz.justpatrick.sync"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#FF0051"
        }
      ],
      "react-native-bottom-tabs",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c616e5ba-f5e6-4e16-9f12-2c748a4b378f"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "owner": "pntx",
    "updates": {
      "url": "https://u.expo.dev/c616e5ba-f5e6-4e16-9f12-2c748a4b378f"
    }
  }
}
